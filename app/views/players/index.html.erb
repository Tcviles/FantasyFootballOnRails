<table>
  <thead>
    <tr>
      <th>Ranking</th>
      <th>Name</th>
      <th>Position</th>
      <th>NFL Team</th>
      <th>ADP</th>
      <th>FP/G</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
    <% @players.each_with_index do |player, index| %>
      <tr>
        <td><%=index+1%></td>
        <td><%=player.name%></td>
        <td><%=player.position.name%></td>
        <td><%=player.nfl_team%></td>
        <td><%=player.adp%></td>
        <td><%=player.ppg%></td>
        <td><%=link_to "Add Me", player_path(player)%></td>
      </tr>
    <% end %>
  </tbody>
</table>
