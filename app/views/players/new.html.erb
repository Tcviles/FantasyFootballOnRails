<%= form_for @player do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :nfl_team %>
  <%= f.text_field :nfl_team %><br>
  <%= f.label :adp %>
  <%= f.text_field :adp %><br>
  <%= f.label :avg_fantasy_ppg, "Average PPG x 10" %>
  <%= f.text_field :avg_fantasy_ppg %><br>
  <%= f.label :position%>
  <%= f.collection_select :position_id, Position.all, :id, :name %><br>
  <%= f.submit %>
<% end %>
